<Project>
  <PropertyGroup>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
  </PropertyGroup>
  <ItemGroup>
    <PackageVersion Include="GirCore.Adw-1" Version="0.3.0" />
    <PackageVersion Include="GirCore.Gtk-4.0" Version="0.3.0" />
    <PackageVersion Include="GirCore.PangoCairo-1.0" Version="0.3.0" />
    <!-- Note: at least 1.4.2-alpha3 is required for fixes to uninstalling addins, from https://github.com/mono/mono-addins/pull/198 -->
    <PackageVersion Include="Mono.Addins" Version="1.4.2-alpha.4" />
    <PackageVersion Include="Mono.Addins.Setup" Version="1.4.2-alpha.4" />
    <PackageVersion Include="Mono.Addins.CecilReflector" Version="1.4.2-alpha.4" />
    <PackageVersion Include="NGettext" Version="0.6.7" />
    <PackageVersion Include="ParagonClipper" Version="6.4.2" />
    <!-- Note: this matches the SharpZipLib version used by Mono.Addins.Setup. Later versions like 1.4.x are incompatible due to some function signature changes -->
    <PackageVersion Include="SharpZipLib" Version="1.3.3" />
    <PackageVersion Include="Tmds.DBus" Version="0.15.0" />

    <!-- Dependencies used for unit tests -->
    <PackageVersion Include="BenchmarkDotNet" Version="0.13.5" />
    <PackageVersion Include="NUnit" Version="3.13.3" />
    <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.6.1" />
    <PackageVersion Include="NUnit3TestAdapter" Version="4.4.2" />
  </ItemGroup>
</Project>
